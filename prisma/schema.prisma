generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model User {
  id String @id @default(cuid()) @map("_id") @db.Uuid
  created_at DateTime? @default(now())
  updated_at DateTime? @default(now())
  userName String
  email String
  photoUrl String
  password String
  count Int
  isActive Boolean

  @@map("users")
}